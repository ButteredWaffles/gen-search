    <TargetFramework>netcoreapp2.0</TargetFramework>
    <GenerateProgramFile>false</GenerateProgramFile>
    <NoWarn>NU1701</NoWarn>
  </PropertyGroup>
  <ItemGroup>
    <None Update="data/mhgen.db">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.NET.Test.Sdk" Version="*"/>
    <PackageReference Include="SQLiteNetExtensions.Async" Version="*"/>
    <PackageReference Include="AngleSharp" Version="*"/>
    <PackageReference Include="sqlite-net-pcl" Version="1.5.166-beta"/>
    <PackageReference Include="xunit" Version="*"/>
    <PackageReference Include="xunit.runner.visualstudio" Version="*"/>
    <PackageReference Include="DeepEqual" Version="*"/>
    <PackageReference Include="AngleSharp.Scripting.JavaScript" Version="*"/>
  </ItemGroup>
</Project>